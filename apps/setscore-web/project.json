{
  "root": "apps/setscore-web",
  "sourceRoot": "apps/setscore-web",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/setscore-web",
        "outputPath": "dist/apps/setscore-web"
      },
      "configurations": {
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "options": {
        "buildTarget": "setscore-web:build",
        "dev": true,
        "port": 3000
      },
      "configurations": {
        "production": {
          "buildTarget": "setscore-web:build:production",
          "dev": false
        }
      }
    },
    "e2e-ci": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "export CYPRESS_BASE_URL='https://setscore-development.vercel.app' && doppler run --token $DOPPLER_SERVICE_TOKEN_DEV --  npx nx run setscore-web-e2e:e2e --configuration ci"
          }
        ]
      }
    },
    "deploy-development": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "doppler run --token $DOPPLER_SERVICE_TOKEN_DEV -- ./tools/deployers/setscore-web.deploy.sh"
          }
        ]
      }
    },
    "deploy-staging": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "doppler run --token $DOPPLER_SERVICE_TOKEN_STAGE -- ./tools/deployers/setscore-web.deploy.sh"
          }
        ]
      }
    },
    "deploy-production": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "doppler run --token $DOPPLER_SERVICE_TOKEN_PROD -- ./tools/deployers/setscore-web.deploy.sh"
          }
        ]
      }
    }
  },
  "tags": []
}
